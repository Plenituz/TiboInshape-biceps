{

  "manifest_version": 2,
  "name": "TiboInShape Bibi Bouton",
  "version": "1.2",

  "description": "Transforme le like en un bibi sur les videos de TiboInShape",

  "icons": {
    "48": "icons/icon-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.youtube.com/*"],
      "js": ["tibofier.js"]
    }
  ],
  
  "web_accessible_resources": [
    "icons/biceps_blue.png",
	"icons/biceps_grey.png"
  ]

}